// Description:
//    Programme minimaliste: une classe et des Cast dans le Main.
//
// 
class A {
    int f() {
        return 0;
    }

    int i() {
        return 1;
    }

    int g() {
        return 2;
    }
}

class B extends A {
    int h() {
        return 3;
    }
    
    int hbis() {
        return 4;
    }

    int i() {
        return 5;
    }

    int j() {
        return 6;
    }

}

{
    A a = new A();
    B b = new B();
    println("On veut 0 : ", a.f());
    println("On veut 1 : ", a.i());
    println("On veut 2 : ", a.g());
    println("On veut 3 : ", b.h());
    println("On veut 4 : ", b.hbis());
    println("On veut 5 : ", b.i());
    println("On veut 6 : ", b.j());
}
