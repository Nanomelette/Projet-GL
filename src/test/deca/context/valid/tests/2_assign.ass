	TSTO #1
	BOV stack_overflow_error
	ADDSP #1
; Main program
	LOAD #1, R2
	STORE R2, 1(GB)
; Beginning of main instructions:
	HALT
; end main program
overflow_error:
	WSTR "Error: overflow_error."
	WNL
	ERROR
