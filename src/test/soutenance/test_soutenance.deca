class Equipe {
    int numeroEquipe;
    Etudiant etudiant0;
    Etudiant etudiant1;
    Etudiant etudiant2;
    Etudiant etudiant3;
    Etudiant etudiant4;

    void setEtudiant(Etudiant e0, Etudiant e1, Etudiant e2, Etudiant e3, Etudiant e4) {
        etudiant0 = e0;
        etudiant1 = e1;
        etudiant2 = e2;
        etudiant3 = e3;
        etudiant4 = e4;
    }

    void setNumeroEquipe(int i) {
        numeroEquipe = i;
    }

    void enumereEquipe() {
        int k = 0;
        Etudiant etudiant;
        while (k < 5) {
            etudiant = getEtudiantI(k);
            etudiant.printFirstName();
            print(" ");
            etudiant.printName();
            println();
            k = k + 1;
        }
    }

    Etudiant getEtudiantI(int i) {
        if (i == 0) {
            return etudiant0;
        } else if (i == 1) {
            return etudiant1;
        } else if (i == 2) {
            return etudiant2;
        } else if (i == 3) {
            return etudiant3;
        } else if (i == 4) {
            return etudiant4;
        } else {
            print("Il n'y a que 5 étudiants dans une équipe !");
        }
    }

    void repartitionEcole() {
        int nbImag = 0;
        int nbPhelma = 0;
        int k = 0;
        Etudiant etudiant;
        while (k < 5) {
            etudiant = getEtudiantI(k);
            if (etudiant.getNumeroEcole() == 1) {
                nbImag = nbImag + 1;
            } else {
                nbPhelma = nbPhelma + 1;
            }
            k = k + 1;
        }
        println("Dans l'équipe GL", numeroEquipe, ", il y a ", nbImag, " étudiants de l'Ensimag et ", nbPhelma, " étudiants de Phelma.");
    }
}

